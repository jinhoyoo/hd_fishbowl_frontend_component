;\nimport { Card } from 'react-bootstrap';\nimport { CardTitle } from 'components';\nimport avatar from 'assets/images/users/avatar-1.jpg';\n\nconst UserProfile = () => {\n    return (\n        <Card>\n            <Card.Body>\n                <CardTitle\n                    containerClass=\"d-flex align-items-center justify-content-between\"\n                    title={\n                        <>\n                            <img className=\"d-flex align-self-start rounded me-2\" src={avatar} alt=\"\" height=\"48\" />\n\n                            <div className=\"w-100 overflow-hidden\">\n                                <h5 className=\"mt-1 mb-0\">Dominic Keller</h5>\n                                <p className=\"mb-1 mt-1 text-muted\">California, USA</p>\n                            </div>\n                        </>\n                    }\n                    icon=\"mdi mdi-dots-horizontal\"\n                    menuItems={[{ label: 'Edit Profile' }, { label: 'Settings' }]}\n                />\n\n                <div className=\"list-group list-group-flush mt-2\">\n                    <Link to=\"#\" className=\"list-group-item list-group-item-action text-primary border-0\">\n                        <i className=\"uil uil-images me-1\"></i> News Feed\n                    </Link>\n                    <Link to=\"#\" className=\"list-group-item list-group-item-action border-0\">\n                        <i className=\"uil uil-comment-alt-message me-1\"></i> Messages\n                    </Link>\n                    <Link to=\"#\" className=\"list-group-item list-group-item-action border-0\">\n                        <i className=\"uil uil-calendar-alt me-1\"></i> Events\n                    </Link>\n                    <Link to=\"#\" className=\"list-group-item list-group-item-action border-0\">\n                        <i className=\"uil uil-users-alt me-1\"></i> Groups\n                    </Link>\n                    <Link to=\"#\" className=\"list-group-item list-group-item-action border-0\">\n                        <i className=\"uil uil-copy me-1\"></i> Pages\n                    </Link>\n                </div>\n            </Card.Body>\n        </Card>\n    );\n};\n\nexport default UserProfile;\n","import { Link } from 'react-router-dom';\nimport { Card } from 'react-bootstrap';\n\nconst Events = () => {\n    return (\n        <Card>\n            <Card.Body className=\"p-2\">\n                <div className=\"list-group list-group-flush my-2\">\n                    <Link to=\"#\" className=\"list-group-item list-group-item-action border-0\">\n                        <i className=\"uil uil-calendar-alt me-1\"></i> 3 events this week\n                    </Link>\n                    <Link to=\"#\" className=\"list-group-item list-group-item-action border-0\">\n                        <i className=\"uil uil-calender me-1\"></i> Eva's birthday today\n                    </Link>\n                    <Link to=\"#\" className=\"list-group-item list-group-item-action border-0\">\n                        <i className=\"uil uil-bookmark me-1\"></i> Jenny's wedding tomorrow\n                    </Link>\n                </div>\n            </Card.Body>\n        </Card>\n    );\n};\n\nexport default Events;\n","import { Link } from 'react-router-dom';\nimport { Card } from 'react-bootstrap';\nimport { Topic } from './types';\nimport { CardTitle } from 'components';\n\nconst News = () => {\n    const news: Topic[] = [\n        {\n            id: 1,\n            title: 'Golden Globes',\n            description: 'The 27 Best moments from the Golden Globe Awards',\n        },\n        {\n            id: 2,\n            title: 'World Cricket',\n            description: 'India has won ICC T20 World Cup Yesterday',\n        },\n        {\n            id: 3,\n            title: 'Antartica',\n            description: 'Metling of Totten Glacier could cause high risk to areas near by sea',\n        },\n    ];\n\n    return (\n        <Card>\n            <Card.Body>\n                <CardTitle\n                    containerClass=\"d-flex align-items-center justify-content-between mb-1\"\n                    title=\"Trending\"\n                    icon=\"mdi mdi-dots-horizontal\"\n                    menuItems={[\n                        { label: 'Today' },\n                        { label: 'Yesterday' },\n                        { label: 'Last Week' },\n                        { label: 'Last Month' },\n                    ]}\n                />\n\n                {news.map((item, index) => {\n                    return (\n                        <div key={index.toString()} className=\"d-flex mt-3\">\n                            <i className=\"uil uil-arrow-growth me-2 font-18 text-primary\"></i>\n                            <div>\n                                <Link className=\"mt-1 font-14\" to=\"#\">\n                                    <strong>{item.title}:</strong>\n                                    <span className=\"text-muted\">{item.description}</span>\n                                </Link>\n                            </div>\n                        </div>\n                    );\n                })}\n            </Card.Body>\n        </Card>\n    );\n};\n\nexport default News;\n","import { FeedPost } from './types';\nimport avatar3 from 'assets/images/users/avatar-3.jpg';\nimport avatar5 from 'assets/images/users/avatar-2.jpg';\nimport avatar9 from 'assets/images/users/avatar-9.jpg';\nimport avatar10 from 'assets/images/users/avatar-5.jpg';\nimport img1 from 'assets/images/small/small-4.jpg';\nimport img2 from 'assets/images/small/small-2.jpg';\nimport img3 from 'assets/images/small/small-3.jpg';\n\nconst posts: FeedPost[] = [\n    {\n        id: 1,\n        author: {\n            id: 1,\n            name: 'Jeremy Tomlinson',\n            avatar: avatar3,\n        },\n        postedOn: 'about 2 minuts ago',\n        scope: 'Public',\n        // tslint:disable-next-line: max-line-length\n        content:\n            '<div class=\"font-16 text-center text-dark my-3\"><i class=\"mdi mdi-format-quote-open font-20\"></i> Leave one wolf alive and the sheep are never safe. When people ask you what happened here, tell them the North remembers. Tell them winter came for House Frey.</div>',\n        totalLikes: '2k',\n        totalComments: '200',\n        isLiked: true,\n        comments: [\n            {\n                id: 1,\n                content: 'This is awesome! Proud of sis :) Waiting for you to come back to winterfall',\n                postedOn: '2 mins ago',\n                author: {\n                    id: 2,\n                    name: 'Sansa Stark',\n                    avatar: avatar9,\n                },\n                isLiked: true,\n                replies: [\n                    {\n                        id: 3,\n                        content: \"I swear! She won't be able to reach to winterfall\",\n                        postedOn: '1 min ago',\n                        author: {\n                            id: 3,\n                            name: 'Cersei Lannister',\n                            avatar: avatar10,\n                        },\n                    },\n                ],\n            },\n        ],\n    },\n    {\n        id: 2,\n        author: {\n            id: 4,\n            name: 'Jon Snow',\n            avatar: avatar5,\n        },\n        postedOn: 'about 20 minuts ago',\n        scope: 'Public',\n        // tslint:disable-next-line: max-line-length\n        content:\n            '<div class=\"my-3\"><p>\"Feeling awesome at the wall!\"</p><div class=\"row\"><div class=\"col-sm-8\">' +\n            '<img src=\"' +\n            img1 +\n            '\" alt=\"post-img\" class=\"rounded me-1 mb-3 mb-sm-0 img-fluid\" /></div><div class=\"col\">' +\n            '<img src=\"' +\n            img2 +\n            '\" alt=\"post-img\" class=\"rounded me-1 img-fluid mb-3\" />' +\n            '<img src=\"' +\n            img3 +\n            '\" alt=\"post-img\" class=\"rounded me-1 img-fluid\" /></div></div></div>',\n        totalLikes: '1.2k',\n        totalComments: '148',\n        isLiked: false,\n        comments: [\n            {\n                id: 1,\n                content: 'This is awesome! Proud of you bro :)',\n                postedOn: '2 mins ago',\n                author: {\n                    id: 2,\n                    name: 'Sansa Stark',\n                    avatar: avatar9,\n                },\n                isLiked: false,\n                replies: [],\n            },\n        ],\n    },\n];\n\nexport default posts;\n","import { Link } from 'react-router-dom';\nimport { Card, Tab, Nav, Dropdown } from 'react-bootstrap';\nimport classnames from 'classnames';\nimport useFeeds from './useFeeds';\nimport { Comment, Person, FeedPost } from './types';\nimport posts from './data';\n\ntype PostCommentProps = {\n    comment: Comment;\n    isActionAllowed: boolean;\n    toggleLikeOnComment: (comment: Comment) => void;\n};\n\ntype PostProps = {\n    post: FeedPost;\n    user: Person;\n    toggleLike: (post: FeedPost) => void;\n    toggleLikeOnComment: (comment: Comment) => void;\n};\n\nconst FormPost = () => {\n    return (\n        <form action=\"#\" className=\"comment-area-box\">\n            <textarea\n                rows={4}\n                className=\"form-control border-0 resize-none\"\n                placeholder=\"Write something....\"\n            ></textarea>\n            <div className=\"p-2 bg-light d-flex justify-content-between align-items-center\">\n                <div>\n                    <Link to=\"#\" className=\"btn btn-sm px-2 font-16 btn-light\">\n                        <i className=\"uil uil-scenery\"></i>\n                    </Link>\n                    <Link to=\"#\" className=\"btn btn-sm px-2 font-16 btn-light\">\n                        <i className=\"uil uil-location\"></i>\n                    </Link>\n                    <Link to=\"#\" className=\"btn btn-sm px-2 font-16 btn-light\">\n                        <i className=\"uil uil-paperclip\"></i>\n                    </Link>\n                </div>\n                <button type=\"submit\" className=\"btn btn-sm btn-success\">\n                    <i className=\"uil uil-message me-1\"></i>Post\n                </button>\n            </div>\n        </form>\n    );\n};\n\nconst createMarkup = (text: string) => {\n    return { __html: text };\n};\n\nconst PostComment = ({ comment, isActionAllowed, toggleLikeOnComment }: PostCommentProps) => {\n    return (\n        <div className=\"mt-3\">\n            <div className=\"d-flex\">\n                <img className=\"me-2 rounded\" src={comment.author.avatar} alt=\"\" height=\"32\" />\n                <div>\n                    <h5 className=\"m-0\">{comment.author.name} </h5>\n                    <p className=\"text-muted mb-0\">\n                        <small>{comment.postedOn}</small>\n                    </p>\n\n                    <p className=\"my-1\" dangerouslySetInnerHTML={createMarkup(comment.content)}></p>\n\n                    {isActionAllowed && (\n                        <div>\n                            <Link\n                                to=\"#\"\n                                className=\"btn btn-sm btn-link text-muted p-0\"\n                                onClick={() => toggleLikeOnComment(comment)}\n                            >\n                                <i\n                                    className={classnames('uil', 'uil-heart', 'me-1', {\n                                        'text-danger': comment.isLiked,\n                                    })}\n                                ></i>{' '}\n                                Like\n                            </Link>\n                            <Link to=\"#\" className=\"btn btn-sm btn-link text-muted p-0 ps-2\">\n                                <i className=\"uil uil-comments-alt me-1\"></i> Reply\n                            </Link>\n                        </div>\n                    )}\n\n                    {comment.replies && (\n                        <>\n                            {comment.replies.map((reply, index) => {\n                                return (\n                                    <PostComment\n                                        key={index.toString()}\n                                        comment={reply}\n                                        isActionAllowed={false}\n                                        toggleLikeOnComment={toggleLikeOnComment}\n                                    />\n                                );\n                            })}\n                        </>\n                    )}\n                </div>\n            </div>\n        </div>\n    );\n};\n\nconst Post = ({ post, user, toggleLike, toggleLikeOnComment }: PostProps) => {\n    return (\n        <Card>\n            <Card.Body className=\"pb-1\">\n                <div className=\"d-flex\">\n                    <img className=\"me-2 rounded\" src={post.author.avatar} alt=\"\" height=\"32\" />\n                    <div className=\"w-100\">\n                        <Dropdown className=\"float-end\" align=\"end\">\n                            <Dropdown.Toggle\n                                variant=\"link\"\n                                className=\"card-drop arrow-none cursor-pointer p-0 shadow-none\"\n                            >\n                                <i className=\"mdi mdi-dots-horizontal\"></i>\n                            </Dropdown.Toggle>\n\n                            <Dropdown.Menu>\n                                <Dropdown.Item>Edit</Dropdown.Item>\n                                <Dropdown.Item>Delete</Dropdown.Item>\n                            </Dropdown.Menu>\n                        </Dropdown>\n\n                        <h5 className=\"m-0\">{post.author.name}</h5>\n                        <p className=\"text-muted\">\n                            <small>\n                                {post.postedOn} <span className=\"mx-1\">⚬</span>\n                                <span>{post.scope}</span>\n                            </small>\n                        </p>\n                    </div>\n                </div>\n\n                <hr className=\"m-0\" />\n\n                <div dangerouslySetInnerHTML={createMarkup(post.content)}></div>\n\n                <hr className=\"m-0\" />\n\n                <div className=\"my-1\">\n                    <Link to=\"#\" className=\"btn btn-sm btn-link text-muted ps-0\" onClick={() => toggleLike(post)}>\n                        <i\n                            className={classnames('mdi', 'mdi-heart', {\n                                'text-danger': post.isLiked,\n                            })}\n                        ></i>{' '}\n                        {post.totalLikes} Likes\n                    </Link>\n                    <Link to=\"#\" className=\"btn btn-sm btn-link text-muted\">\n                        <i className=\"uil uil-comments-alt me-1\"></i>\n                        {post.totalComments} Comments\n                    </Link>\n                    <Link to=\"#\" className=\"btn btn-sm btn-link text-muted\">\n                        <i className=\"uil uil-share-alt me-1\"></i>\n                        Share\n                    </Link>\n                </div>\n\n                <hr className=\"m-0\" />\n\n                <div className=\"mt-3\">\n                    {(post.comments || []).map((comment, index) => {\n                        return (\n                            <PostComment\n                                key={index.toString()}\n                                comment={comment}\n                                isActionAllowed={true}\n                                toggleLikeOnComment={toggleLikeOnComment}\n                            ></PostComment>\n                        );\n                    })}\n                </div>\n\n                <hr />\n\n                {user && (\n                    <div className=\"d-flex mb-2\">\n                        <img src={user.avatar} height=\"32\" className=\"align-self-start rounded me-2\" alt=\"\" />\n                        <div className=\"w-100\">\n                            <input\n                                type=\"text\"\n                                className=\"form-control border-0 form-control-sm\"\n                                placeholder=\"Write a comment\"\n                            />\n                        </div>\n                    </div>\n                )}\n            </Card.Body>\n        </Card>\n    );\n};\n\nconst LoadMore = () => {\n    return (\n        <div className=\"text-center mb-3\">\n            <Link to=\"#\" className=\"text-danger\">\n                <i className=\"mdi mdi-spin mdi-loading me-1 font-16\"></i> Load more\n            </Link>\n        </div>\n    );\n};\n\nconst Feeds = () => {\n    const { user, toggleLike, toggleLikeOnComment } = useFeeds();\n    return (\n        <>\n            <Tab.Container defaultActiveKey=\"post\">\n                <Card>\n                    <Card.Body className=\"p-0\">\n                        <Nav variant=\"tabs\" className=\"nav nav-tabs nav-bordered\" as=\"ul\">\n                            <Nav.Item as=\"li\">\n                                <Nav.Link href=\"#\" eventKey=\"post\">\n                                    <i className=\"mdi mdi-pencil-box-multiple font-18 d-md-none d-block\"></i>\n                                    <span className=\"d-none d-md-block\">Create Post</span>\n                                </Nav.Link>\n                            </Nav.Item>\n                            <Nav.Item as=\"li\">\n                                <Nav.Link href=\"#\" eventKey=\"video\">\n                                    <i className=\"mdi mdi-image font-18 d-md-none d-block\"></i>\n                                    <span className=\"d-none d-md-block\">Photos/Videos</span>\n                                </Nav.Link>\n                            </Nav.Item>\n                            <Nav.Item as=\"li\">\n                                <Nav.Link href=\"#\" eventKey=\"story\">\n                                    <i className=\"mdi mdi-book-open-variant font-18 d-md-none d-block\"></i>\n                                    <span className=\"d-none d-md-block\">Story</span>\n                                </Nav.Link>\n                            </Nav.Item>\n                        </Nav>\n\n                        <Tab.Content>\n                            <Tab.Pane eventKey=\"post\" className=\"p-3\">\n                                <div className=\"border rounded\">\n                                    <FormPost />\n                                </div>\n                            </Tab.Pane>\n                            <Tab.Pane eventKey=\"video\" className=\"p-3\">\n                                <div className=\"border rounded\">\n                                    <FormPost />\n                                </div>\n                            </Tab.Pane>\n                            <Tab.Pane eventKey=\"story\" className=\"p-3\">\n                                <div className=\"border rounded\">\n                                    <FormPost />\n                                </div>\n                            </Tab.Pane>\n                        </Tab.Content>\n                    </Card.Body>\n                </Card>\n            </Tab.Container>\n\n            {posts.map((post, index) => {\n                return (\n                    <Post\n                        post={post}\n                        key={index.toString()}\n                        user={user}\n                        toggleLike={toggleLike}\n                        toggleLikeOnComment={toggleLikeOnComment}\n                    />\n                );\n            })}\n\n            <LoadMore></LoadMore>\n        </>\n    );\n};\n\nexport default Feeds;\n","import { useState } from 'react';\nimport { Comment, Person, FeedPost } from './types';\nimport avatar3 from 'assets/images/users/avatar-1.jpg';\n\nexport default function useFeeds() {\n    const [user] = useState<Person>({ id: 1, avatar: avatar3 });\n\n    /*\n     * toggle like on post\n     */\n    const toggleLike = (post: FeedPost) => {\n        post.isLiked = !post.isLiked;\n    };\n\n    /*\n     * toggle like on comment\n     */\n    const toggleLikeOnComment = (comment: Comment) => {\n        comment.isLiked = !comment.isLiked;\n    };\n\n    return {\n        user,\n        toggleLike,\n        toggleLikeOnComment,\n    };\n}\n","import { Card } from 'react-bootstrap';\nimport { CardTitle } from 'components';\n\nconst FeaturedVideo = () => {\n    return (\n        <>\n            <Card>\n                <Card.Body>\n                    <CardTitle\n                        containerClass=\"d-flex align-items-center justify-content-between mb-1\"\n                        title=\"Featured Video For You\"\n                        icon=\"mdi mdi-dots-horizontal\"\n                        menuItems={[\n                            { label: 'Today' },\n                            { label: 'Yesterday' },\n                            { label: 'Last Week' },\n                            { label: 'Last Month' },\n                        ]}\n                    />\n\n                    <div className=\"mt-3\">\n                        <div className=\"ratio ratio-16x9\">\n                            <iframe\n                                className=\"embed-responsive-item\"\n                                title=\"video\"\n                                src=\"https://www.youtube.com/embed/9_eqq0HlN9g?autohide=0&showinfo=0&controls=0\"\n                            ></iframe>\n                        </div>\n                    </div>\n                </Card.Body>\n            </Card>\n        </>\n    );\n};\n\nexport default FeaturedVideo;\n","import { Card } from 'react-bootstrap';\nimport { Link } from 'react-router-dom';\nimport { CardTitle } from 'components';\nimport { Person } from './types';\nimport avatar3 from 'assets/images/users/avatar-3.jpg';\nimport avatar4 from 'assets/images/users/avatar-4.jpg';\nimport avatar5 from 'assets/images/users/avatar-2.jpg';\nimport avatar9 from 'assets/images/users/avatar-9.jpg';\nimport avatar10 from 'assets/images/users/avatar-5.jpg';\n\nconst Followers = () => {\n    const peopleToFollow: Person[] = [\n        {\n            id: 1,\n            name: 'Robb Stark',\n            status: 'The first king in the North',\n            avatar: avatar5,\n        },\n        {\n            id: 2,\n            name: 'Sansa Stark',\n            status: 'Lady of winterfall',\n            avatar: avatar9,\n        },\n        {\n            id: 3,\n            name: 'Cersei Lannister',\n            status: 'Queen of the Seven Kingdoms',\n            avatar: avatar10,\n        },\n        {\n            id: 4,\n            name: 'Daenerys Targaryen',\n            status: 'The dragon queen',\n            avatar: avatar3,\n        },\n        {\n            id: 5,\n            name: 'Adhamd Annaway',\n            status: 'I am available!',\n            avatar: avatar4,\n        },\n    ];\n\n    return (\n        <Card>\n            <Card.Body>\n                <CardTitle\n                    containerClass=\"d-flex align-items-center justify-content-between mb-3\"\n                    title=\"People you may know\"\n                    icon=\"mdi mdi-dots-horizontal\"\n                    menuItems={[\n                        { label: 'Today' },\n                        { label: 'Yesterday' },\n                        { label: 'Last Week' },\n                        { label: 'Last Month' },\n                    ]}\n                />\n\n                <div className=\"inbox-widget\">\n                    {peopleToFollow.map((people, index) => {\n                        return (\n                            <div key={index.toString()} className=\"inbox-item\">\n                                <div className=\"inbox-item-img\">\n                                    <img src={people.avatar} className=\"rounded-circle\" alt=\"\" />\n                                </div>\n                                <p className=\"inbox-item-author\">{people.name}</p>\n                                <p className=\"inbox-item-text\">{people.status}</p>\n                                <p className=\"inbox-item-date\">\n                                    <button type=\"button\" className=\"btn btn-sm btn-outline-primary px-1 py-0\">\n                                        {' '}\n                                        <i className=\"uil uil-user-plus font-16\"></i>{' '}\n                                    </button>\n                                </p>\n                            </div>\n                        );\n                    })}\n                </div>\n\n                <div className=\"mt-2 mb-3 text-center\">\n                    <Link to=\"#\">\n                        View More<i className=\"uil uil-arrow-right ms-1\"></i>\n                    </Link>\n                </div>\n            </Card.Body>\n        </Card>\n    );\n};\n\nexport default Followers;\n","import { Row, Col } from 'react-bootstrap';\nimport { PageTitle } from 'components';\nimport UserProfile from './UserProfile';\nimport Events from './Events';\nimport News from './News';\nimport Feeds from './Feeds';\nimport FeaturedVideo from './FeaturedVideo';\nimport Followers from './Followers';\n\nconst SocialFeed = () => {\n    return (\n        <>\n            <PageTitle\n                breadCrumbItems={[\n                    { label: 'Apps', path: '/apps/social' },\n                    { label: 'Social Feed', path: '/apps/social', active: true },\n                ]}\n                title={'Social Feed'}\n            />\n\n            <Row>\n                <Col xxl={3} lg={6} className=\"order-lg-1 order-xxl-1\">\n                    <UserProfile />\n                    <Events />\n                    <News />\n                </Col>\n\n                <Col xxl={6} lg={12} className=\"order-lg-2 order-xxl-1\">\n                    <Feeds />\n                </Col>\n\n                <Col xxl={3} lg={6} className=\"order-lg-1 order-xxl-2\">\n                    <FeaturedVideo />\n                    <Followers />\n                </Col>\n            </Row>\n        </>\n    );\n};\n\nexport { SocialFeed };\n"],"names":["Card","containerClass","title","className","src","avatar","alt","height","icon","menuItems","label","to","id","description","map","item","index","toString","author","name","avatar3","postedOn","scope","content","totalLikes","totalComments","isLiked","comments","avatar9","replies","avatar10","avatar5","img1","img2","img3","FormPost","action","rows","placeholder","type","createMarkup","text","__html","PostComment","comment","isActionAllowed","toggleLikeOnComment","dangerouslySetInnerHTML","onClick","classnames","reply","Post","post","user","toggleLike","Dropdown","align","variant","LoadMore","useState","useFeeds","Tab","defaultActiveKey","Nav","as","href","eventKey","posts","peopleToFollow","status","avatar4","people","SocialFeed","breadCrumbItems","path","active","Row","Col","xxl","lg"],"sourceRoot":""}